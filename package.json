{
  "publisher": "fxzer",
  "name": "theme-vitesse-dark-custom",
  "displayName": "Vitesse Dark Custom",
  "version": "0.0.1",
  "packageManager": "pnpm@8.7.1",
  "description": "Vitesse Dark Custom is a custom theme baseed on Vitesse Dark for VS Code",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/fxzer/theme-vitesse-dark-custom"
  },
  "bugs": {
    "url": "https://github.com/fxzer/theme-vitesse-dark-custom/issues"
  },
  "keywords": [
    "theme",
    "vitesse",
    "dark",
    "custom"
  ],
  "categories": [
    "Themes"
  ],
  "icon": "icon.png",
  "files": [
    "themes"
  ],
  "engines": {
    "vscode": "^1.43.0"
  },
  "contributes": {
    "themes": [
      {
        "label": "Vitesse Dark Custom",
        "uiTheme": "vs-dark",
        "path": "./themes/vitesse-dark-custom.json"
      }
    ]
  },
  "scripts": {
    "build": "esno src/index.ts",
    "dev": "nr clean && esno watch src/index.ts",
    "clean": "rm -rf themes",
    "vscode:prepublish": "npm run build",
    "lint": "eslint .",
    "prepublishOnly": "npm run build",
    "release": "bumpp --all && vsce publish --no-dependencies"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.41.4",
    "@antfu/utils": "^0.7.6",
    "@types/node": "^18.18.6",
    "@vscode/vsce": "^2.21.1",
    "bumpp": "^9.2.0",
    "eslint": "^8.52.0",
    "esno": "^0.17.0",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  },
  "preview": true
}
